{
  "name": "ts-dynamic-array",
  "version": "1.0.1",
  "description": "Maintain a large array of data in a small memory footprint",
  "type": "module",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "start": "node dist/bundle.cjs.js",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/e-hosseini/ts-dynamic-array.git"
  },
  "keywords": [
    "dynamic array",
    "TypeScript",
    "large array management",
    "array focus index",
    "memory-efficient array",
    "array manipulation",
    "memory optimization"
  ],
  "author": "e-hosseini",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
